package tut00;

public class Var {

	public static void main(String[] args) {
		// 데이터타입 이름 = 값;
		// a = b : b를 a에 저장
		// a == b : a 와 b의 메모리 값이 같다 
		// a.equals("b") : a와 b의 값이 같다
//		int num = 100;
//		System.out.println(num);
		
		int num1 = 10;
		int num2 = 20;
		
		System.out.println(num1 + num2);
	}

}
