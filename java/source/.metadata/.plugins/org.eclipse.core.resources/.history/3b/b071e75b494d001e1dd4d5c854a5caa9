package tut00;

//클래스 이름은 Hello 파일이름과 동일해야 함
public class Hello {

	public static void main(String[] args) {
		// 한글자는 문자 '', 한글자 이상은 문자열 ""
		
		// :(콜론)=> 계속 이어질 뭔가 있는것
		// ;(세미콜론) => 문자 끝, 프로그램이 끝
//		System.out.println("Hello");
//		System.out.println("World");
		System.out.print("Hello");
		System.out.println("World");
		
//		문자열 "abvad"
//		System.out.println("a");
	}

}
